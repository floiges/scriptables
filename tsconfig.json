{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "lib": [
      "esnext",
      "dom"
    ],
    "allowJs": true,
    "suppressImplicitAnyIndexErrors": true,
    "baseUrl": "./Scripts",
  },
  "include": [
    "Scripts/**/*.js",
    "Scripts/**/*.d.ts",
  ]
}